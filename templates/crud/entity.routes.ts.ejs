import { AI1_TITLE_SUFFIX } from 'app/app.routes';
import { <%= entityPascal %>EditComponent } from './edit/edit.component';
import { <%= entityPascal %>Component } from './<%= entityKebab %>.component';
import { <%= entityPascal %>ListComponent } from './list/list.component';

const <%= entityConstant %> = '<%= entityCapital %>';

export default [
    {
        path: '',
        redirectTo: 'list',
        pathMatch: 'full',
    },
    {
        path: '',
        component: <%= entityPascal %>Component,
        children: [
            {
                path: 'list',
                component: <%= entityPascal %>ListComponent,
                title: `${<%= entityConstant %>}: List` + AI1_TITLE_SUFFIX,
                data: {
                    breadcrumb: {
                        label: 'List',
                        info: 'heroicons_outline:queue-list',
                    },
                },
            },
            {
                path: 'add',
                component: <%= entityPascal %>EditComponent,
                title: `${<%= entityConstant %>}: Add` + AI1_TITLE_SUFFIX,
                data: {
                    breadcrumb: {
                        label: 'Add',
                        info: 'heroicons_outline:plus',
                    },
                },
            },
            {
                path: 'edit/:id',
                component: <%= entityPascal %>EditComponent,
                title: `${<%= entityConstant %>}: Edit` + AI1_TITLE_SUFFIX,
                data: {
                    breadcrumb: {
                        label: 'Edit',
                        info: 'heroicons_outline:pencil-square',
                    },
                },
            },
        ],
    },
];
